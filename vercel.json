{
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npx prisma generate && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ]
}
